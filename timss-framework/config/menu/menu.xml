<?xml version="1.0" encoding="UTF-8"?>
<!-- 系统菜单配置文件 -->
<!-- menus为菜单配置参数，attr总：id表示菜单id（需要和数据库sec_menu的id对应）, name表示菜单名称, web表示指向页面, 
	show显示标记：show显示，hide隐藏默认为show 考虑将样式过滤层jsp也放在这里配置 -->
<menuconfig name="">
	<menus id="modoid" name="模块名称" web="menupage.do">
		<menu id="menuid1" name="菜单名称" web="menu.do" show="show">
			<menu id="childmenuid1" name="菜单名称1" web="childmenu1.do" show="show" />
		</menu>
		<menu id="menuid1" name="菜单名称" web="menu.do" show="show">
			<menu id="childmenuid1" name="菜单名称1" web="childmenu1.do" show="show" />
		</menu>
	</menus>
	<menus id="modoid" name="模块名称" web="menupage.do">
		<menu id="menuid1" name="菜单名称" web="menu.do" show="show">
			<menu id="childmenuid1" name="菜单名称1" web="childmenu1.do" show="show" />
		</menu>
		<menu id="menuid1" name="菜单名称" web="menu.do" show="show">
			<menu id="childmenuid1" name="菜单名称1" web="childmenu1.do" show="show" />
		</menu>
	</menus>
</menuconfig>